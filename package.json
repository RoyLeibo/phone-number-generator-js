{
  "name": "phone-number-generator-js",
  "version": "1.2.16",
  "description": "Generates a random phone number following the E.164 international standard",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "example",
    "typescript",
    "npm",
    "package",
    "sample",
    "boilerplate",
    "starter",
    "demo",
    "cjs",
    "esm",
    "umd"
  ],
  "license": "MIT",
  "addscope": "@RoyLeibo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoyLeibo/phone-number-generator-js.git"
  },
  "author": "Roy Leibovitz",
  "bugs": {
    "url": "https://github.com/RoyLeibo/phone-number-generator-js/issues"
  },
  "homepage": "https://github.com/RoyLeibo/phone-number-generator-js#readme",
  "dependencies": {
    "class-validator": "0.14.1",
    "libphonenumber-js": "1.10.30",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.194",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "pinst": "2.1.6",
    "prettier": "2.4.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "typescript": "4.4.3",
    "webpack": "5.82.1",
    "webpack-cli": "4.8.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
